<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" maxWidth="1.7976931348623157E308" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.InputSettingsController">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox>
               <children>
                  <Label text="Luck charm" />
                  <Region HBox.hgrow="ALWAYS" />
                  <Label fx:id="lblLuckValue" text="0" />
               </children>
            </HBox>
            <Slider fx:id="slidLuck" onMouseClicked="#onLuckDragDone" onMouseDragged="#onLuckDragDone" prefWidth="190.0" showTickMarks="true" snapToTicks="true" />
            <HBox>
               <children>
                  <Label text="Risk" />
                  <Region HBox.hgrow="ALWAYS" />
                  <Label fx:id="lblRiskValue" text="0" />
               </children>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </HBox>
            <Slider fx:id="slidRisk" onMouseClicked="#onRiskDragDone" onMouseDragged="#onRiskDragDone" showTickMarks="true" snapToTicks="true" />
            <HBox>
               <children>
                  <Label text="Fail train" />
                  <Region HBox.hgrow="ALWAYS" />
                  <Label fx:id="lblFailValue" text="0" />
               </children>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </HBox>
            <Slider fx:id="slidFail" onMouseClicked="#onFailDragDone" onMouseDragged="#onFailDragDone" showTickMarks="true" snapToTicks="true" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
   <padding>
      <Insets top="10.0" />
   </padding>
</AnchorPane>
